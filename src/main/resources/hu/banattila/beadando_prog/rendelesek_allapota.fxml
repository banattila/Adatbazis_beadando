<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: bisque;"
            xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hu.banattila.beadando_prog.controllers.RendelesekAllapotaController">
    <children>
        <TableView fx:id="tw" prefHeight="450.0" prefWidth="652.0">
            <columns>
                <TableColumn fx:id="rendeloEmail" prefWidth="231.0" text="Rendelő email címe"/>
                <TableColumn fx:id="rendelesIdeje" minWidth="0.0" prefWidth="222.0" text="Rendelés ideje"/>
                <TableColumn fx:id="rendelesAllapot" prefWidth="198.0" text="Rendelés állapota"/>
            </columns>
        </TableView>
        <AnchorPane layoutY="450.0" prefHeight="318.0" prefWidth="652.0" style="-fx-background-color: antiquewhite;">
            <Label layoutX="88.0" layoutY="33.0" text="A kijelölt rendelés állapotának teljesítettnek minősítése">
                <font>
                    <Font size="17.0"/>
                </font>
            </Label>
            <Label layoutX="152.0" layoutY="60.0" text="Csak teljesített értékre lehet változtatni.">
                <font>
                    <Font size="17.0"/>
                </font>
            </Label>
            <Label layoutX="107.0" layoutY="98.0" text="A rendeléshez tartozó futár ezután elérhetővé válik">
                <font>
                    <Font size="17.0"/>
                </font>
            </Label>
            <Button fx:id="rendelesTeljesit" layoutX="273.0" layoutY="143.0" mnemonicParsing="false" text="Teljesített">
                <font>
                    <Font size="17.0"/>
                </font>
            </Button>
            <Button fx:id="refresh" layoutX="243.0" layoutY="227.0" mnemonicParsing="false" text="Adatok frissítése">
                <font>
                    <Font size="17.0"/>
                </font>
            </Button>
        </AnchorPane>
    </children>
</AnchorPane>
